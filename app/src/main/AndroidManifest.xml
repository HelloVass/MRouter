<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chiclaim.modularization">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ActivityManagerActivity"
            android:label="ActivityManager"
            android:screenOrientation="portrait" />

        <activity
            android:name=".range.AActivity"
            android:label="AActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".range.BActivity"
            android:label="BActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".range.CActivity"
            android:label="CActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".range.DActivity"
            android:label="DActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name=".NoAutowiredActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name=".TargetActivity"
            android:label="TargetActivity"
            android:screenOrientation="portrait" />

    </application>

</manifest>